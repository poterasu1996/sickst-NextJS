{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "brand": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "rating": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 5
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "products"
    },
    "retail_value": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "required": true
    },
    "otb_price": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "quantity": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "stripe_fullpriceLink": {
      "type": "string",
      "required": true
    },
    "subscription_type": {
      "type": "enumeration",
      "enum": [
        "mystery",
        "basic",
        "premium"
      ],
      "required": true
    },
    "product_name": {
      "type": "string",
      "required": true
    }
  }
}
