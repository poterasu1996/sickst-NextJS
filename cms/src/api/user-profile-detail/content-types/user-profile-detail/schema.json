{
  "kind": "collectionType",
  "collectionName": "user_profile_details",
  "info": {
    "singularName": "user-profile-detail",
    "pluralName": "user-profile-details",
    "displayName": "user_profile_detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "default": "Sickst"
    },
    "last_name": {
      "type": "string",
      "default": "User"
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "another"
      ]
    },
    "address": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "county": {
      "type": "string"
    },
    "zip_code": {
      "type": "integer"
    },
    "phone_number": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "user_id": {
      "type": "integer",
      "required": true
    },
    "reviews": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "client_role": {
      "type": "enumeration",
      "enum": [
        "client",
        "admin"
      ],
      "required": false,
      "default": "client"
    },
    "newsletter": {
      "type": "boolean",
      "default": true
    },
    "new_user": {
      "type": "boolean",
      "default": true
    },
    "subscribed": {
      "type": "boolean",
      "default": false
    },
    "products_received": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "product_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-review.product-review",
      "mappedBy": "user_profile_detail"
    },
    "subscription_name": {
      "type": "enumeration",
      "enum": [
        "mystery",
        "basic",
        "premium"
      ]
    }
  }
}
