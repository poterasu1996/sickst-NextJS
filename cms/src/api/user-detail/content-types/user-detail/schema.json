{
  "kind": "collectionType",
  "collectionName": "user_details",
  "info": {
    "singularName": "user-detail",
    "pluralName": "user-details",
    "displayName": "user_details",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "first_name": {
      "type": "string",
      "default": "Sickst",
      "required": true
    },
    "birthday": {
      "type": "date"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Male",
        "Female"
      ],
      "default": "Female"
    },
    "last_name": {
      "type": "string",
      "default": "User",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "city": {
      "type": "string"
    },
    "county": {
      "type": "string"
    },
    "zip_code": {
      "type": "integer"
    },
    "phone_number": {
      "type": "string",
      "maxLength": 10,
      "required": false,
      "unique": false
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "user_id": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "reviews": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "products_received": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "product_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-review.product-review",
      "mappedBy": "user_details"
    }
  }
}
