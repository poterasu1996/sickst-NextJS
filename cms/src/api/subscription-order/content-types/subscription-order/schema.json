{
  "kind": "collectionType",
  "collectionName": "subscription_orders",
  "info": {
    "singularName": "subscription-order",
    "pluralName": "subscription-orders",
    "displayName": "subscription_order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "integer",
      "required": true
    },
    "expire_date": {
      "type": "date",
      "required": false
    },
    "txn_status": {
      "type": "enumeration",
      "enum": [
        "success",
        "pending",
        "canceled"
      ],
      "required": true
    },
    "subscription_list": {
      "type": "json"
    },
    "session_id": {
      "type": "string",
      "required": true
    },
    "subscription_price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "subscription_name": {
      "type": "enumeration",
      "enum": [
        "mystery",
        "basic",
        "premium"
      ],
      "required": true
    },
    "subscription_status": {
      "type": "enumeration",
      "enum": [
        "active",
        "pending",
        "cancelled"
      ],
      "required": true,
      "default": "pending"
