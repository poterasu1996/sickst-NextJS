{
  "kind": "collectionType",
  "collectionName": "subscription_orders",
  "info": {
    "singularName": "subscription-order",
    "pluralName": "subscription-orders",
    "displayName": "subscription_order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "integer",
      "required": true
    },
    "expire_date": {
      "type": "date",
      "required": false
    },
    "txn_status": {
      "type": "enumeration",
      "enum": [
        "success",
        "pending",
        "failed",
        "cancelled"
      ],
      "required": true
    },
    "subscription_list": {
      "type": "json"
    },
    "session_id": {
      "type": "string",
      "required": true
    },
    "subscription_price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "subscription_name": {
      "type": "enumeration",
      "enum": [
        "mystery",
        "basic",
        "premium"
      ],
      "required": true
    },
    "last_payment_date": {
      "type": "date"
    },
    "subscription_status": {
      "type": "enumeration",
      "enum": [
        "active",
        "paused",
        "cancelled"
      ]
    },
    "cancelled_subscription": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cancelled-subscription.cancelled-subscription",
      "inversedBy": "subscription_order"
    },
    "is_cancelled": {
      "type": "boolean",
      "default": false
    }
  }
}
