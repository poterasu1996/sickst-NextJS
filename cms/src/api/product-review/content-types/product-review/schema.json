{
  "kind": "collectionType",
  "collectionName": "product_reviews",
  "info": {
    "singularName": "product-review",
    "pluralName": "product-reviews",
    "displayName": "product_review",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_id": {
      "type": "integer",
      "required": true
    },
    "title_review": {
      "type": "string",
      "required": false
    },
    "review": {
      "type": "text",
      "required": false
    },
    "rating": {
      "type": "integer",
      "required": true,
      "max": 5,
      "min": 1
    },
    "likes": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "dislikes": {
      "type": "integer",
      "min": 0,
      "default": 0
    },
    "user_details": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user-detail.user-detail",
      "inversedBy": "product_reviews"
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "users_liked": {
      "type": "json"
    },
    "users_disliked": {
      "type": "json"
    }
  }
}
